{
  "name": "mitydigital/iconamic",
  "description": "The SVG icon selector fieldtype and tag for Statamic.",
  "license": "MIT",
  "type": "statamic-addon",
  "autoload": {
    "psr-4": {
      "MityDigital\\Iconamic\\": "src"
    }
  },
  "extra": {
    "statamic": {
      "name": "Iconamic",
      "description": "The SVG Icon Selector for Statamic"
    },
    "laravel": {
      "providers": [
        "MityDigital\\Iconamic\\ServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "statamic/cms": "^4.0 || ^5.0"
  },
  "autoload-dev": {
    "psr-4": {
      "MityDigital\\Iconamic\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.9|^7.1",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit"
    ]
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
